{
  "name": "is-datatype",
  "version": "0.3.1",
  "description": "Type validation function meant to go beyond the use cases of operators such as `typeof` by providing a few more common pseudo data types to check against, as well as a certain degree of options to validate against as well",
  "main": "dist/bundle/is.func.umd.min.js",
  "types": "dist/is.func.d.ts",
  "scripts": {
    "precommit": "lint-staged",
    "commitmsg": "validate-commit-msg",
    "pretest": "npm run clean:test",
    "test": "node_modules/.bin/karma start --single-run",
    "pretest:watch": "npm run pretest",
    "test:watch": "node_modules/.bin/karma start --no-single-run",
    "test-ci": "npm run build && npm run test && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "serve:test": "node_modules/.bin/browser-sync start -s coverage/html",
    "clean:test": "node_modules/.bin/rimraf coverage",
    "clean:dist": "node_modules/.bin/rimraf dist",
    "clean": "npm run clean:test & npm run clean:dist",
    "dev": "npm run clean:dist && nodemon --watch 'src' -e 'ts' -x 'node_modules/.bin/tsc'",
    "format:base": "node_modules/.bin/prettier --print-width 120 --no-semi --single-quote --parser typescript --write",
    "format": "npm run format:base -- \"src/**/*.ts\"",
    "prebuild": "npm run clean:dist",
    "build:compile": "node_modules/.bin/tsc",
    "build:format": "npm run format:base -- \"dist/*.js\"",
    "build:bundle": "node_modules/.bin/webpack --env.min && node_modules/.bin/webpack",
    "build": "npm run build:compile && npm run build:format && npm run build:bundle",
    "release": "./release-check.sh && node_modules/.bin/np && npm logout"
  },
  "repository": {
    "type": "git",
    "url": "git+https://emilio-martinez@github.com/emilio-martinez/is-datatype.git"
  },
  "keywords": [
    "validation",
    "typescript",
    "data",
    "type"
  ],
  "author": "Emilio Martinez-Cordero <e.marticor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emilio-martinez/is-datatype/issues"
  },
  "homepage": "https://github.com/emilio-martinez/is-datatype#readme",
  "devDependencies": {
    "@types/jasmine": "~2.6.0",
    "@types/node": "^8.0.30",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "compression-webpack-plugin": "^1.0.1",
    "coveralls": "^3.0.0",
    "husky": "^0.14.3",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-typescript": "^3.0.6",
    "lint-staged": "^4.2.2",
    "nodemon": "^1.12.1",
    "np": "^2.16.0",
    "prettier": "^1.7.0",
    "rimraf": "^2.6.2",
    "ts-loader": "^2.3.7",
    "tslib": "^1.7.1",
    "typescript": "^2.5.2",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^3.6.0"
  }
}
