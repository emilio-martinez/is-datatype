{
  "name": "is-datatype",
  "version": "0.3.1",
  "description": "Type validation function meant to go beyond the use cases of operators such as `typeof` by providing a few more common pseudo data types to check against, as well as a certain degree of options to validate against as well",
  "sideEffects": false,
  "main": "dist/bundle/is.func.umd.min.js",
  "types": "dist/is.func.d.ts",
  "scripts": {
    "commitmsg": "commitlint -e",
    "lint:tslint:src": "node_modules/.bin/tslint --format verbose --project tsconfig.json",
    "lint:tslint:spec": "node_modules/.bin/tslint --format verbose --project tsconfig.spec.json spec/**/*.spec.ts",
    "lint:tslint": "npm run lint:tslint:src && npm run lint:tslint:spec",
    "lint": "npm run lint:tslint",
    "format:base": "node_modules/.bin/prettier --write *.ts **/*.ts !dist/**/*.ts",
    "format:write": "npm run format:base -- --write",
    "format:check": "npm run format:base -- --list-different",
    "prebenchmark": "npm run clean && npm run build",
    "benchmark": "node tools/benchmarks/index.js",
    "pretest": "npm run clean:test",
    "test": "npm run lint && npm run format:check && node_modules/.bin/karma start --single-run",
    "pretest:watch": "npm run pretest",
    "test:watch": "node_modules/.bin/karma start --no-single-run",
    "serve:test": "node_modules/.bin/browser-sync start -s coverage/html",
    "clean:test": "node_modules/.bin/rimraf coverage",
    "clean:dist": "node_modules/.bin/rimraf dist",
    "clean": "npm run clean:test & npm run clean:dist",
    "dev": "npm run clean:dist && node_modules/.bin/tsc --watch",
    "prebuild": "npm run clean:dist",
    "build:compile": "node_modules/.bin/tsc",
    "build:bundle": "node_modules/.bin/webpack --env.min && node_modules/.bin/webpack",
    "build": "npm run build:compile && npm run build:bundle",
    "release": "./release-check.sh && node_modules/.bin/np && npm logout"
  },
  "repository": {
    "type": "git",
    "url": "git+https://emilio-martinez@github.com/emilio-martinez/is-datatype.git"
  },
  "keywords": [
    "validation",
    "typescript",
    "data",
    "type"
  ],
  "author": "Emilio Martinez-Cordero <e.marticor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emilio-martinez/is-datatype/issues"
  },
  "homepage": "https://github.com/emilio-martinez/is-datatype#readme",
  "devDependencies": {
    "@commitlint/cli": "6.2.0",
    "@commitlint/config-angular": "6.1.3",
    "@types/benchmark": "1.0.31",
    "@types/jasmine": "2.8.7",
    "@types/node": "8.10.17",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.7.0",
    "benchmark": "2.1.4",
    "chalk": "2.4.1",
    "compression-webpack-plugin": "1.1.11",
    "coveralls": "3.0.1",
    "husky": "0.14.3",
    "jasmine-core": "3.1.0",
    "jsdom": "11.11.0",
    "karma": "2.0.2",
    "karma-jasmine": "1.1.2",
    "karma-jsdom-launcher": "6.1.3",
    "karma-typescript": "3.0.12",
    "nodemon": "1.17.5",
    "np": "2.20.1",
    "prettier": "1.13.0",
    "rimraf": "2.6.2",
    "ts-loader": "4.3.0",
    "ts-node": "6.0.5",
    "tslib": "1.9.1",
    "tslint": "5.10.0",
    "tslint-config-standard": "7.0.0",
    "tslint-no-redundant-jsdoc-annotations": "1.0.1",
    "typescript": "2.8.3",
    "webpack": "4.9.2",
    "webpack-cli": "2.1.4"
  }
}
