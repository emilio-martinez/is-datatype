{
  "name": "is-datatype",
  "version": "0.3.1",
  "description": "Type validation function meant to go beyond the use cases of operators such as `typeof` by providing a few more common pseudo data types to check agasint, as well as a certain degree of options to validate against as well",
  "main": "dist/bundle/is.func.umd.min.js",
  "types": "dist/is.func.d.ts",
  "scripts": {
    "pretest": "npm run clean:test",
    "test": "node_modules/.bin/karma start --single-run",
    "pretest:watch": "npm run pretest",
    "test:watch": "node_modules/.bin/karma start --no-single-run",
    "test-ci": "npm run build && npm run test && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "serve:test": "node_modules/.bin/browser-sync start -s coverage/html",
    "clean:test": "node_modules/.bin/rimraf coverage",
    "clean:dist": "node_modules/.bin/rimraf dist",
    "clean": "npm run clean:test & npm run clean:dist",
    "prebuild": "npm run clean:dist",
    "build": "node_modules/.bin/tsc",
    "postbuild": "node_modules/.bin/webpack --env.min && node_modules/.bin/webpack",
    "release": "./release-check.sh && node_modules/.bin/np && npm logout"
  },
  "repository": {
    "type": "git",
    "url": "git+https://emilio-martinez@github.com/emilio-martinez/is-datatype.git"
  },
  "keywords": [
    "validation",
    "typescript",
    "data",
    "type"
  ],
  "author": "Emilio Martinez-Cordero <e.marticor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emilio-martinez/is-datatype/issues"
  },
  "homepage": "https://github.com/emilio-martinez/is-datatype#readme",
  "devDependencies": {
    "@types/jasmine": "^2.5.47",
    "@types/node": "^7.0.21",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.0",
    "coveralls": "^2.13.1",
    "jasmine-core": "^2.6.2",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-typescript": "^3.0.2",
    "np": "^2.15.0",
    "rimraf": "^2.6.0",
    "ts-loader": "^2.0.3",
    "typescript": "~2.3.2",
    "uglify-js": "^3.0.10",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "webpack": "^2.5.1"
  }
}
