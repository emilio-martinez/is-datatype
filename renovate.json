{
  "extends": [
    "config:base",
    "schedule:weekly",
    ":semanticCommitTypeAll(build)",
    ":semanticCommitScope(package)",
    ":automergeMinor"
  ],
  "commitMessage": "{{semanticPrefix}}update {{depName}} to version {{newVersion}}",
  "packageRules": [
    {
      "packagePatterns": ["^@commitlint\/.*"],
      "groupName": "commitlint"
    }
  ]
}
