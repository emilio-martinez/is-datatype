{
    "defaultSeverity": "error",
    "extends": [],
    "jsRules": {},
    "rules": {
        "arrow-return-shorthand": [true, "multiline"],
        "curly": [true, "ignore-same-line"],
        "eofline": true,
        "import-spacing": true,
        "interface-over-type-literal": true,
        "linebreak-style": [true, "LF"],
        "max-line-length": [true, 120],
        "no-consecutive-blank-lines": true,
        "no-console": true,
        "no-construct": true,
        "no-debugger": true,
        "no-duplicate-imports": true,
        "no-duplicate-variable": [true, "check-parameters"],
        "no-inferrable-types": true,
        "no-irregular-whitespace": true,
        "no-non-null-assertion": true,
        "no-object-literal-type-assertion": true,
        "no-parameter-reassignment": true,
        "no-string-throw": true,
        "no-unnecessary-initializer": true,
        "no-unnecessary-type-assertion": true,
        "no-var-keyword": true,
        "number-literal-format": true,
        "object-literal-sort-keys": [true, "ignore-case"],
        "ordered-imports": [true, { "import-sources-order": "any", "named-imports-order": "case-insensitive" }],
        "prefer-conditional-expression": true,
        "prefer-const": true,
        "radix": true,
        "strict-boolean-expressions": [true, "allow-undefined-union", "allow-boolean-or-undefined"],
        "triple-equals": true,
        "typedef-whitespace": true,
        "typeof-compare": true,
        "unified-signatures": true,
        "use-isnan": true,
        "variable-name": [true, "ban-keywords", "check-format"]
    },
    "rulesDirectory": []
}