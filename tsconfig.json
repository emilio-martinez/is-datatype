{
  "compilerOptions": {
    "module": "es2015",
    "target": "es2015",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "moduleResolution": "node",
    "noEmitHelpers": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": false,
    "sourceMap": false,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@lib": [
        "src/lib/public-api.ts"
      ],
      "@lib-private": [
        "src/lib/private-api.ts"
      ]
    }
  }
}